<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cafe.ahmed.cafemenu">

    <application
        android:allowBackup="true"
        android:icon="@drawable/order"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ChoseDrink"
            android:label="Chose Drink"/>
        <activity android:name=".MenuCoffe"
            android:label="Menu Coffee"/>
        <activity android:name=".OrderSummary"
            android:label="Order Summary"/>
        <activity android:name=".MenuTea"
            android:label="Menu Tea"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".MenuJuice"
            android:label="Menu Juice"></activity>
    </application>

</manifest>